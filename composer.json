{
  "name": "metamorph/metamorph",
  "type": "library",
  "description": "Transformation library",
  "keywords": ["transformer"],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "nikic/php-parser": "^4.0@dev",
    "zendframework/zend-config-aggregator": "^1.1@dev",
    "zendframework/zend-config": "^3.2@dev"
  },
  "require-dev": {
    "codeception/codeception": "^2.3.8",
    "fzaninotto/faker": "^1.8@dev",
    "nesbot/carbon": "dev-master",
    "ramsey/uuid": "^3.0@dev"
  },
  "suggest": {
    "nesbot/carbon": "Date transformations",
    "ramsy/uuid": "Uuid transformation"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Metamorph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Acceptance\\": "tests/acceptance",
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
