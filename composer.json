  "keywords": ["transformer"],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "nikic/php-parser": "^4.0@dev"
  },
  "require-dev": {
    "codeception/codeception": "^2.3.8",
    "ramsey/uuid": "^3.0@dev"
  },
  "minimum-stability": "dev",
  "autoload": {
